<%= form_for @course do |f| %>
  <div>
    <%= f.label :course_name %>
    <%= f.text_field :course_name%>
  </div>
  <div>
    <%= f.label :course_teacher %>
    <%= f.text_field :course_teacher%>
  </div>
  <div>
    <%= f.label :course_time %>
    <%= f.text_field :course_time%>
  </div>
  <div>
    <%= f.label :course_place %>
    <%= f.text_field :course_place%>
  </div>
  <div>
    <%= f.label :course_credit %>
    <%= f.text_field :course_credit%>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>